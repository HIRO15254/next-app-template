{
  "name": "next-app-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "yarn prisma:generate && next build && yarn prisma:migrate-deploy",
    "codegen": "yarn prisma:generate && graphql-codegen --config codegen.cts",
    "dev": "yarn local-db:start && yarn prisma:generate && yarn prisma:migrate-dev && graphql-codegen --config codegen.cts && next dev",
    "fix": "gts fix && npm run lint:stylelint --fix",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "lint": "gts lint && yarn lint:stylelint",
    "lint:stylelint": "stylelint \"**/*.css\"",
    "local-db:start": "docker-compose -f .dev/docker-compose.yml up -d",
    "local-db:stop": "docker-compose -f .dev/docker-compose.yml down",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "prisma-accelerate-local": "prisma-accelerate-local postgres://postgres:passw0rd@localhost:5433/nat_local -p 4000",
    "prisma:generate": "prisma generate --schema=./src/lib/prisma/schema.prisma --no-engine",
    "prisma:migrate-deploy": "prisma migrate deploy --schema=./src/lib/prisma/schema.prisma",
    "prisma:migrate-dev": "prisma migrate dev --schema=./src/lib/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/lib/prisma/schema.prisma",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "npm run prettier:check && npm run lint && npm run typecheck && npm run jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.10.3",
    "@apollo/experimental-nextjs-app-support": "^0.10.1",
    "@apollo/server": "^4.10.4",
    "@as-integrations/next": "^3.0.0",
    "@auth/prisma-adapter": "^2.0.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@mantine/core": "7.9.1",
    "@mantine/form": "^7.9.1",
    "@mantine/hooks": "7.9.1",
    "@mantine/notifications": "^7.9.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/bundle-analyzer": "^14.2.3",
    "@pothos/core": "^3.41.1",
    "@pothos/plugin-errors": "^3.11.1",
    "@pothos/plugin-prisma": "^3.65.1",
    "@pothos/plugin-relay": "^3.46.0",
    "@pothos/plugin-validation": "^3.10.1",
    "@prisma/client": "^5.13.0",
    "@prisma/extension-accelerate": "^1.0.0",
    "@tabler/icons-react": "^3.3.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.23.0",
    "gts": "^5.3.0",
    "mantine-form-zod-resolver": "^1.1.0",
    "next": "14.2.3",
    "next-auth": "^5.0.0-beta.17",
    "next-pwa": "^5.6.0",
    "prisma": "^5.13.0",
    "prisma-accelerate-local": "^1.1.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "webpack": "^5.91.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@jest/globals": "^29.7.0",
    "@next/eslint-plugin-next": "^14.2.3",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/blocks": "^8.0.10",
    "@storybook/nextjs": "^8.0.10",
    "@storybook/react": "^8.0.10",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20.12.11",
    "@types/pg": "^8.11.6",
    "@types/react": "18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-mantine": "3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "storybook": "^8.0.10",
    "storybook-addon-apollo-client": "^7.3.0",
    "storybook-dark-mode": "^4.0.1",
    "stylelint": "^16.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.4.5"
  },
  "packageManager": "yarn@4.1.1"
}
